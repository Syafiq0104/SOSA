<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signInScreenoneVM" type="com.syafiqaqilahsapplication.app.modules.signinscreenone.data.viewmodel.SignInScreenoneVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumn"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_900">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameSignin"
                android:layout_height="match_parent" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnwelcome"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:paddingBottom="@dimen/_96pxv"
                    android:paddingEnd="@dimen/_38pxh"
                    android:paddingStart="@dimen/_38pxh"
                    android:paddingTop="@dimen/_96pxv" style="@style/groupStylegray_400_89cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtWelcome"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="wrap_content"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{signInScreenoneVM.signInScreenoneModel.txtWelcome}"
                        style="@style/txtPoppinsbold20_1" tools:text="@string/lbl_welcome"/>
                    <ImageView android:id="@+id/imageUndrawmyappOne"
                        android:layout_height="@dimen/_172pxh"
                        android:layout_marginBottom="@dimen/_54pxv"
                        android:layout_marginTop="@dimen/_54pxv"
                        android:layout_width="@dimen/_229pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_undraw_my_app_grf2"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_undraw_my_app_grf2"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumnskinbiops"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearRow"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <LinearLayout android:gravity="end"
                            android:id="@+id/linearRowskinbiopsyth"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_100pxv"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageSkinbiopsythree"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_51pxh"
                                android:layout_width="@dimen/_52pxh"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_skin_biopsy_1}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_skin_biopsy_1"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearColumnprice"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginStart="@dimen/_8pxh"
                                android:layout_width="wrap_content" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtPrice"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{signInScreenoneVM.signInScreenoneModel.txtPrice}"
                                    style="@style/txtPoppinsbold25" tools:text="@string/lbl_sosa"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtSolarsafe"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{signInScreenoneVM.signInScreenoneModel.txtSolarsafe}"
                                    style="@style/txtPoppinsregular15" tools:text="@string/lbl_solar_safe"/>
                            </LinearLayout>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageImage"
                            android:layout_height="@dimen/_168pxh"
                            android:layout_marginStart="@dimen/_55pxh"
                            android:layout_width="@dimen/_149pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_group_2}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_group_2"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameStackemail"
                        android:layout_height="@dimen/_396pxv"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_313pxv" android:layout_width="@dimen/_355pxh">
                        <FrameLayout
                            android:id="@+id/frameStackskinbiopsy"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_347pxv" android:layout_width="@dimen/_355pxh">
                            <ImageView
                                android:id="@+id/imageSkinbiopsythree1"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_347pxh"
                                android:layout_width="match_parent"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_skin_biopsy_3}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_skin_biopsy_3"/>
                            <LinearLayout
                                android:id="@+id/linearColumnpassword"
                                android:layout_gravity="top|center"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_23pxh"
                                android:layout_marginStart="@dimen/_23pxh"
                                android:layout_marginTop="@dimen/_41pxv"
                                android:layout_width="match_parent" android:orientation="vertical">
                                <EditText
                                    android:background="@drawable/rectangle_bg_white_a700_radius_20"
                                    android:ellipsize="end"
                                    android:hint="@string/msg_enter_your_password"
                                    android:id="@+id/etPassword"
                                    android:inputType="text"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:paddingBottom="@dimen/_20pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_20pxv"
                                    android:singleLine="true"
                                    android:text="@={signInScreenoneVM.signInScreenoneModel.etPasswordValue}"
                                    android:textColorHint="@color/black_900_70"
                                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtForgotpassword"
                                    android:layout_gravity="center_horizontal"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_24pxv"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{signInScreenoneVM.signInScreenoneModel.txtForgotpassword}"
                                    style="@style/txtRobotobold18" tools:text="@string/lbl_forgot_password"/>

                                <androidx.appcompat.widget.AppCompatButton
                                    android:ellipsize="end"
                                    android:gravity="center"
                                    android:id="@+id/btnSignIn"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_28pxv"
                                    android:layout_width="match_parent"
                                    android:paddingBottom="@dimen/_14pxv"
                                    android:paddingEnd="@dimen/_30pxh"
                                    android:paddingStart="@dimen/_30pxh"
                                    android:paddingTop="@dimen/_14pxv"
                                    android:singleLine="true"
                                    android:text="@string/lbl_sign_in"
                                    android:textAllCaps="false"
                                    style="@style/btnSolidRounded" tools:text="@string/lbl_sign_in"/>
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtConfirmation"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_15pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{signInScreenoneVM.signInScreenoneModel.txtConfirmation}"
                                    style="@style/txtRobotobold18" tools:text="@string/msg_dont_have_an_account"/>
                            </LinearLayout>
                        </FrameLayout>
                        <EditText
                            android:background="@drawable/rectangle_bg_white_a700_radius_20"
                            android:ellipsize="end"
                            android:hint="@string/msg_enter_your_email"
                            android:id="@+id/etEmail"
                            android:inputType="text"
                            android:layout_gravity="top|center"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:paddingBottom="@dimen/_20pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_20pxv"
                            android:singleLine="true"
                            android:text="@={signInScreenoneVM.signInScreenoneModel.etEmailValue}"
                            android:textColorHint="@color/black_900_70"
                            style="@style/etSolidRounded" tools:ignore="Autofill"/>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </LinearLayout>
</layout>
